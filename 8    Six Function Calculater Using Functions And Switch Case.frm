Private Sub Command1_Click()
Select Case op
Case "+"
C = Val(A) + Val(B)
Print "Answer="; C
Case "-"
C = Val(A) - Val(B)
Print "Answer="; C
Case "*"
C = Val(A) * Val(B)
Print "Answer="; C
Case "/"
C = Val(A) / Val(B)
Print "Answer="; C
Case "MOD"
C = Val(A) Mod Val(B)
Print "Answer="; C
Case "^"
C = Val(A) ^ Val(B)
Print "Answer="; C
Case Else
Print "Operation Not Valid"
End Select
End Sub

